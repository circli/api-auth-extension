{
    "name": "circli/api-auth-extension",
    "type": "circli-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Sundqvist",
            "email": "andreas@oak-valley.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "php-di/php-di": "^6.0 || ^7.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "circli/auth-extension": "dev-master",
        "psr/log": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~9.0",
        "phpstan/phpstan": "^1.12.0"
    },
    "autoload": {
        "psr-4": {
            "Circli\\ApiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
